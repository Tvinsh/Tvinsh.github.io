<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title><%= config.title %></title>
	<meta name="keywords" content="tomato的博客，tomato，最帅的博客，前端博客，javascript,html,css,nodejs,pwa，amp">
	<meta name="description" content="tomato的博客，tomato，最帅的博客，前端博客，javascript,html,css,nodejs,pwa，amp">
	<meta name="viewport" content="user-scalable=no,width=718">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="baidu-site-verification" content="HY5hBj9CX6" />
	<meta name="robots" content="all">
    <meta name="googlebot" content="all">
    <meta name="baiduspider" content="all">
    <meta name="format-detection" content="telephone=no">
    <link rel="manifest" href="/manifest.json">
    <% if (is_post() && config.generator_amp) { %>
	  <link rel="amphtml" href="./amp/index.html">
	<% } %>
	<link rel="alternate" type="application/rss+xml" title="RSS 2.0" href="rss">
	<link rel="stylesheet" href="https://cdn.bootcss.com/highlight.js/9.2.0/styles/github.min.css">
	<script src="https://cdn.bootcss.com/highlight.js/9.2.0/highlight.min.js"></script>
	<%- css('css/i.css') %>
	<%- css('css/cmts.css') %>
	<%- css('css/arc.0.2.css') %>
	<link rel="canonical" href="<%- page.permalink %>">
	<script type="text/javascript">
		var host = "2fz1.me";
		if ((host == window.location.host) && (window.location.protocol != "https:"))
			window.location.protocol = "https";
	</script>
</head>
<body>
	<%- partial('_partial/nav') %>
	<div id="m">
		<%- partial('_partial/search') %>
		<% var aflag = is_current('/tags') && !is_current('/tags/demo') && !is_current('/tags/life') && !is_current('/tags/photo'); %>
		<% if(aflag) { %>
			<div class="content">
				<% site.tags.map(function(tag, index){  %>
				<div class="module module-toplist">
					<a href="/tags/<%= tag.name %>" class="book-toplist a<%= index %>" title="<%= tag.name %>" rel="<%= tag.length %>">
						<h2 class="book-toplist-title"><%= tag.name %></h2>
					</a>
					<ol class="book-ol book-ol-rank">
						<% tag.posts.map(function(post){  %>
							<li class="book-li">
								
									<a class="book-layout" href="<%- config.root %><%- post.path %>">
										<h3 class="book-title">
											<%- post.title || "Untitled" %>
										</h3>
									</a>
								
							</li>
						<% }) %>
					</ol>
				</div>
				<% }) %>  
			</div>
		<% } %>
		<%- body %>
	</div>
</body>
<script async src="//dn-lbstatics.qbox.me/busuanzi/2.3/busuanzi.pure.mini.js"></script>
<script>
	hljs.initHighlightingOnLoad();
	// 注册service worker
	if (navigator.serviceWorker != null) {
      navigator.serviceWorker.register('/service-worker.js')
      .then(function(registration) {
        console.log('Registered events at scope: ', registration.scope);
      });
    }
</script>
</html>